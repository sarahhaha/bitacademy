    "print('This program prints the multiple of 3 from 1 to 100')\n",
    "one_to_hundred = []\n",
    "multiple_of_three = []\n",
    "for i in range(1,101,1):\n",
    "    one_to_hundred.append(i)\n",
    "\n",
    "for number in one_to_hundred:\n",
    "    if number % 3 == 0:\n",
    "        multiple_of_three.append(number)\n",
    "    \n",
    "print(multiple_of_three)"
